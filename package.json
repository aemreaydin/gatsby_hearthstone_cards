    "eslint-plugin-import": "^2.22.0",
    "firebase": "^7.17.2",
    "gatsby": "^2.24.26",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-chakra-ui": "^0.1.4",
    "gatsby-plugin-css-modules-typings": "^1.0.1",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-typescript": "^2.4.17",

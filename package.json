  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
